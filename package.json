{
    "name": "ra-language-chinese",
    "version": "2.0.5",
    "description": "Chinese messages for react-admin, the frontend framework for building admin applications on top of REST/GraphQL services",
    "main": "index.js",
    "module": "esm/index.js",
    "types": "esm/index.d.ts",
    "scripts": {
        "build": "yarn run build-cjs && yarn run build-esm",
        "build-cjs": "rimraf ./lib && tsc",
        "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
        "watch": "tsc --outDir esm --module es2015 --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chen4w/ra-language-chinese.git"
    },
    "keywords": [
        "react",
        "react-admin",
        "react-admin",
        "translation"
    ],
    "author": "c4w",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/chen4w/ra-language-chinese/issues"
    },
    "homepage": "https://github.com/chen4w/ra-language-chinese#readme",
    "dependencies": {
        "ra-core": "^3.4.1"
    },
    "devDependencies": {
        "rimraf": "^3.0.2"
    }
}
