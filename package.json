{
    "name": "ra-language-chinese",
    "version": "3.0.0",
    "description": "chinese messages for react-admin, the frontend framework for building admin applications on top of REST/GraphQL services",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chen4w/ra-language-chinese.git"
    },
    "files": [
        "*.md",
        "dist",
        "src"
    ],
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/cjs/index.d.ts",
    "scripts": {
        "build": "pnpm run build-cjs && pnpm run build-esm",
        "build-cjs": "rimraf ./dist/cjs && tsc --outDir dist/cjs",
        "build-esm": "rimraf ./dist/esm && tsc --outDir dist/esm --module es2015",
        "watch": "tsc --outDir dist/esm --module es2015 --watch"
    },
    "dependencies": {
        "ra-core": "^4.7.0"
    },
    "devDependencies": {
        "rimraf": "^3.0.2",
        "typescript": "^4.4.0"
    },
    "keywords": [
        "react",
        "react-admin",
        "admin-on-rest",
        "translation"
    ],
    "author": "c4w",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/chen4w/ra-language-chinese/issues"
    },
    "homepage": "https://github.com/chen4w/ra-language-chinese#readme"
}
